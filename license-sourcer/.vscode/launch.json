    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run License Builder",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/src/index.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": ["${workspaceFolder}/dist/**/*.js"],
            "env": {
                "RUN_SCRIPT": "license_builder"
            }
        },
        {
            "name": "Run Component Finder",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/src/index.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": ["${workspaceFolder}/dist/**/*.js"],
            "env": {
                "RUN_SCRIPT": "component_finder"
            }
        },
        {
            "name": "Run More Data Finder",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/src/index.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": ["${workspaceFolder}/dist/**/*.js"],
            "env": {
                "RUN_SCRIPT": "more_data_finder"
            }
        },
        {
            "name": "Run License Text Cleaner",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/src/index.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": ["${workspaceFolder}/dist/**/*.js"],
            "env": {
                "RUN_SCRIPT": "license_text_cleaner"
            }
        },
        {
            "name": "Parse SPDX Feed",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/src/index.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": ["${workspaceFolder}/dist/**/*.js"],
            "env": {
                "RUN_SCRIPT": "parse_spdx_feed"
            }
        },
        {
            "name": "Run Final",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/src/index.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": ["${workspaceFolder}/dist/**/*.js"],
            "env": {
                "RUN_SCRIPT": "run_final"
            }
        }
    ]
}
